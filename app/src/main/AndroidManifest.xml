<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.intsig.yann.analysis">
    <uses-permission android:name="android.permission.CAMERA"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.DarkActionBar">
        <provider
            android:name="com.intsig.yann.analysis.AnalysisDataProvider"
            android:authorities="com.intsig.yann.analysis.provider"
            android:multiprocess="false"
            android:exported="false">
        </provider>
        <activity android:name="com.intsig.yann.analysis.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.intsig.yann.analysis.AnalysisDetailActivity"
            android:label="详情页">

        </activity>
    </application>

</manifest>